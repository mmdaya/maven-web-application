/* groovylint-disable CompileStatic */
pipeline {
    agent any
    tools {
        maven 'maven-3.9.2'
    }
    stages {
        stage('Build_Maven') {
            steps {
                sh '''
                mvn clean package
                '''
            }
        }
            stage('Deploy_Package') {
                steps {
                    // sh '''
                    // cd target
                    // ls
                    // '''
                     script{
deploy adapters: [tomcat9(credentialsId: 'Tomcat-Manager-Deploy', path: '', url: 'http://3.7.68.232:8080/')], contextPath: null, war: '/target/maven-web-application.war'            }
    }
}
}