/* groovylint-disable CompileStatic */
pipeline {
    agent any
    tools {
        maven 'maven-3.9.2'
    }
    stages {
        stage('Build_Maven') {
            steps {
                sh '''
                mvn clean package
                '''
            }
        }
            stage('Deploy_Package') {
                steps {
                    sh '''
                    cd ./target
                    ls
                    '''
                }
            }
    }
}